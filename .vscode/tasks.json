{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compilar C com gcc",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-Wall",
                "-Wextra",
                "-g3",
                "${workspaceFolder}/Dicionário_hash/Dicionario_com_Hash/main.c",
                "-o",
                "${workspaceFolder}/Dicionário_hash/Dicionario_com_Hash/output/main"
            ],
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
